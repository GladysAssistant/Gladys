{
  "name": "gladys-server",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "scripts": {
    "postinstall": "node ./cli/install_service_dependencies.js",
    "test": "cross-env NODE_ENV=test ./node_modules/mocha/bin/mocha --recursive ./test/bootstrap.test.js \"./test/**/*.test.js\" --exit",
    "test-service": "cross-env SQLITE_FILE_PATH=/tmp/gladys-test.db NODE_ENV=test ./node_modules/mocha/bin/mocha --recursive ./test/bootstrap.test.js \"./test/services/$npm_config_service/*.test.js\" \"./test/services/$npm_config_service/**/*.test.js\" --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov",
    "prettier-check": "prettier --check '**/*.js' '**/*.json'",
    "prettier": "prettier --write '**/*.js' '**/*.json'",
    "eslint-fix": "eslint . --fix",
    "eslint": "eslint .",
    "start": "cross-env NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "db-reset:dev": "cross-env NODE_ENV=development node_modules/.bin/sequelize db:migrate:undo:all",
    "db-migrate:dev": "cross-env NODE_ENV=development node_modules/.bin/sequelize db:migrate",
    "db-migrate:prod": "cross-env NODE_ENV=production node_modules/.bin/sequelize db:migrate",
    "db-seed:dev": "cross-env NODE_ENV=development node_modules/.bin/sequelize db:seed:all",
    "db-reset-seed:dev": "cross-env NODE_ENV=development node_modules/.bin/sequelize db:seed:undo:all",
    "db-migrate:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "jsdoc": "jsdoc lib/**/*.js -d jsdoc",
    "apidoc": "apidoc -i controllers/ -o apidoc/",
    "checkjs": "tsc --allowJs --resolveJsonModule --checkJs --noEmit --lib ES5,ES2018 lib/**/*.js api/**/*.js services/**/*.js",
    "generate-jsdoc": "jsdoc lib/**/*.js services/**/*.js -d jsdoc",
    "cypress": "npm run cypress:clean && npm run cypress:start",
    "cypress:start": "cross-env SQLITE_FILE_PATH=./gladys-cypress.db node index.js",
    "cypress:clean": "npx rimraf ./gladys-cypress.db*"
  },
  "author": "Pierre-Gilles Leymarie",
  "license": "Apache-2.0",
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "persist/*"
    ]
  },
  "nyc": {
    "exclude": [
      "migrations/**/*.js",
      "test/**/*.js"
    ]
  },
  "devDependencies": {
    "apidoc": "^1.0.3",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^43.1.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-no-call": "^1.2.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-require-jsdoc": "^1.0.4",
    "jsdoc": "^3.6.10",
    "mocha": "^8.3.2",
    "mock-express-request": "^0.2.2",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^1.17.1",
    "proxyquire": "^2.1.0",
    "shx": "^0.3.2",
    "sinon": "^7.2.4",
    "supertest": "^3.4.2"
  },
  "dependencies": {
    "@gladysassistant/gladys-gateway-js": "^4.12.0",
    "@hapi/joi": "^17.1.0",
    "@hapi/joi-date": "^2.0.1",
    "@nlpjs/similarity": "^4.26.1",
    "async-retry": "^1.3.3",
    "axios": "^1.2.0",
    "bcrypt": "^5.1.0",
    "bluebird": "^3.7.2",
    "bottleneck": "^2.19.5",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.6",
    "dockerode": "^3.3.4",
    "downsample": "^1.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "form-data": "^2.3.3",
    "fs-extra": "^11.1.0",
    "get-value": "^3.0.1",
    "handlebars": "^4.7.7",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "mathjs": "^11.8.0",
    "node-nlp": "^4.26.1",
    "node-schedule": "^2.1.0",
    "path-to-regexp": "^6.2.1",
    "pify": "^5.0.0",
    "queue": "^6.0.2",
    "rate-limiter-flexible": "^3.0.2",
    "semver": "^7.3.8",
    "sequelize": "^6.26.0",
    "sequelize-cli": "^6.5.2",
    "set-value": "^4.1.0",
    "sqlite3": "^5.1.2",
    "suncalc": "^1.9.0",
    "tracer": "^1.1.6",
    "umzug": "^2.2.0",
    "uuid": "^9.0.0",
    "ws": "^6.2.2"
  }
}
